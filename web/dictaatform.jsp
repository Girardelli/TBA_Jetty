<%@ page import = "java.util.*,
be.tba.util.constants.*"
%>


<%!
private static final String kSelected = "selected";
private static final String kChecked = "checked";
%>


<%
Vector vErrorList = (Vector) request.getAttribute(Constants.ERROR_VECTOR);
String vName = (String) request.getParameter(Constants.FORM_NAME);
if (vName == null) vName = "";

String vCompany = (String) request.getParameter(Constants.FORM_COMPANY);
if (vCompany == null) vCompany = "";

String vEmail = (String) request.getParameter(Constants.FORM_EMAIL);
if (vEmail == null) vEmail = "";

String vTel = (String) request.getParameter(Constants.FORM_TEL);
if (vTel == null) vTel = "";

String vFax = (String) request.getParameter(Constants.FORM_FAX);
if (vFax == null) vFax = "";

String vBrand = (String) request.getParameter(Constants.FORM_DIC_BRAND);
if (vBrand == null) vBrand = "";

String vDuration = (String) request.getParameter(Constants.FORM_DURATION);
if (vDuration == null) vDuration = "";

String vText = (String) request.getParameter(Constants.FORM_TEXT);
if (vText == null) vText = "";

String vLanguage = (String) request.getParameter(Constants.FORM_LANGUAGE);
if (vLanguage == null) vLanguage = "dutch";

String vTechnology = (String) request.getParameter(Constants.FORM_DICTAAT_TECH);
if (vTechnology == null) vTechnology = "digital";

%>



<td valign="top" width="580" bgcolor="FFFFFF">
  <br>
  <span class="bodytitle"> Aanvraag voor dictaat prijsofferte</span>
  <br>
  <p>
  <span class="bodytekst">De veldjes gemarkeerd met een </span><span class="bodyredbold">*</span><span class="bodytekst"> moeten zeker ingevuld worden.</p>
  <%
        if (vErrorList != null && vErrorList.size() > 0)
        {
          out.println("<br><span class=\"bodyredbold\">");
          for (Iterator i = vErrorList.iterator(); i.hasNext();)
          {
            out.println("<img src=\"/tba/images/blueVink.gif\" border=\"0\">" + (String) i.next() + "<br>");
          }
          out.println("</span>");
        }
  %>
  <form name="dictaatForm" method="POST" action="/tba/Login">
    <p class="adminsubtitle"><img src=".\images\blueSphere.gif" width="10" height="10">&nbsp;Uw contact gegevens:</p>
    <table width="570" border="0" cellspacing="2" cellpadding="2">
      <tr>
        <td width="20" ></td>
        <td width="200" valign="top" class="adminsubsubtitle">Aanspreek naam <span class="bodyredbold">*</span></td>
        <td width="340" valign="top"><input type=text name=<%=Constants.FORM_NAME%> size=50 value="<%=vName%>"></td>
      </tr>
      <tr>
        <td width="20" ></td>
        <td width="200" valign="top" class="adminsubsubtitle">Bedrijf</td>
        <td width="340" valign="top"><input type=text name=<%=Constants.FORM_COMPANY%> size=50 value="<%=vCompany%>"></td>
      </tr>
      <tr>
        <td width="20" ></td>
        <td width="200" valign="top" class="adminsubsubtitle">E-mail</td>
        <td width="340" valign="top"><input type=text name=<%=Constants.FORM_EMAIL%> size=50 value="<%=vEmail%>"></td>
      </tr>
      <tr>
        <td width="20" ></td>
        <td width="200" valign="top" class="adminsubsubtitle">Telefoon <span class="bodyredbold">*</span></td>
        <td width="340" valign="top"><input type=text name=<%=Constants.FORM_TEL%> size=50 value="<%=vTel%>"></td>
      </tr>
      <tr>
        <td width="20" ></td>
        <td width="200" valign="top" class="adminsubsubtitle">Fax</td>
        <td width="340" valign="top"><input type=text name=<%=Constants.FORM_FAX%> size=50 value="<%=vFax%>"></td>
      </tr>
    </table>
    <p class="adminsubtitle"><img src=".\images\blueSphere.gif" width="10" height="10">&nbsp;Dictaat informatie:</p>
    <table width="570" border="0" cellspacing="2" cellpadding="2">
      <tr>
        <td width="20" ></td>
        <td width="200" valign="top" class="adminsubsubtitle">Type dictaat</td>
        <td width="340" valign="top" class="adminsubsubtitle">
          <input type=radio name=<%=Constants.FORM_DICTAAT_TECH%> value="digital" <%=(vTechnology.equals("digital")?kChecked:"")%> > digitaal<br>
          <input type=radio name=<%=Constants.FORM_DICTAAT_TECH%> value="analog" <%=(vTechnology.equals("analog")?kChecked:"")%> > analoog<br>
        </td>
      </tr>
      <tr>
        <td width="20" ></td>
        <td width="200" valign="top" class="adminsubsubtitle">Merk en type uw dictafoon <span class="bodyredbold">*</span></td>
        <td width="340" valign="top"><input type=text name=<%=Constants.FORM_DIC_BRAND%> size=50 value="<%=vBrand%>"></td>
      </tr>
      <tr>
        <td width="20" ></td>
        <td width="200" valign="top" class="adminsubsubtitle">Taal <span class="bodyredbold">*</span></td>
        <td width="340" valign="top">
          <select name=<%=Constants.FORM_LANGUAGE%>>
            <option value="dutch" <%=(vLanguage.equals("dutch")?kSelected:"")%> >Nederlands
            <option value="english" <%=(vLanguage.equals("english")?kSelected:"")%> >Engels
            <option value="german" <%=(vLanguage.equals("german")?kSelected:"")%> >Duits
            <option value="french" <%=(vLanguage.equals("french")?kSelected:"")%> >Frans
            <option value="spanish" <%=(vLanguage.equals("spanish")?kSelected:"")%> >Spaans
            <option value="other" <%=(vLanguage.equals("other")?kSelected:"")%> >andere taal
          </select>
      </tr>
      <tr>
        <td width="20" ></td>
        <td width="200" valign="top" class="adminsubsubtitle">Lengte in minuten <span class="bodyredbold">*</span></td>
        <td width="340" valign="top"><input type=text name=<%=Constants.FORM_DURATION%> size=20 value="<%=vDuration%>"></td>
      </tr>
    </table>
    <p class="adminsubtitle"><img src=".\images\blueSphere.gif" width="10" height="10">&nbsp;Bijkomende informatie:</p>
    <table width="570" border="0" cellspacing="2" cellpadding="2">
      <tr>
        <td width="20" ></td>
        <td width="540" valign="top"><textarea name=<%=Constants.FORM_TEXT%> rows=10 cols=70><%=vText%></textarea></td>
      </tr>
    </table>
    <input class="tbabutton" type=reset value=" Blad Wissen ">
    <input type=hidden name=<%=Constants.SRV_ACTION%> value="<%=Constants.ACTION_DICTAAT_FORM%>">
    <input class="tbabutton" type=submit name=action value=" Verstuur ">
  </form>
  <br>
</td>
        </tr>
      </table>
    </td>
  </tr>
</table>

